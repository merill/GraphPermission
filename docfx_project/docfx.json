{
  "build": {
    "content": [
      {
        "files": [
          "permission/**.md",
          "permission/**/toc.yml",
          "about/**.md",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "sitemap":{
      "baseUrl": "https://graphpermissions.merill.net",
      "priority": 0.1,
      "changefreq": "daily",
      "fileOptions":{
          "**/*.md": {
              "baseUrl": "https://graphpermissions.merill.net",
              "priority": 0.8,
              "changefreq": "daily"
          }
      }
    },    
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "globalMetadata": {
      "_appTitle": "Graph Permissions",
      "_enableSearch": false
    },
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": true
  }
}